#fload "../lib/libs.sf"
#fload "helpers.sf"

(SECTION "SFC arithmetics") ;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;

(test (/ 4) '(/ 4) 0.25)
(test (/ 1 4) '(/ 1 4) 0.25)

(test (* 4 (/ 4)) '(* 4 (/ 4)) 1.0)
(test (* (/ 4) 4) '(* (/ 4) 4) 1.0)

(test (* 4 (/ 4)) '(* 4 (/ 4)) 1)
(test (* (/ 4) 4) '(* (/ 4) 4) 1)

(test (* 4 (/ 1 4)) '(* 4 (/ 1 4)) 1)
(test (* (/ 1 4) 4) '(* (/ 1 4) 4) 1)

(test (* 4 0.25) '(* 4 0.25) 1.0)

(define (main argv) (report-errors))
